{
  "extends": ["universe/native", "plugin:react-hooks/recommended"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "caughtErrors": "none", "ignoreRestSiblings": true }
    ],
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "type",
          "object"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
