{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "ts-node": "TS_NODE_TRANSPILE_ONLY=true ts-node",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,json\" --exec \"npm run ts-node ./src/index.ts\"",
    "build": "npm run lint && NODE_ENV=production tsc",
    "start": "npm run build && PORT=8080 nodemon --watch \"./dist/**\" -x 'node ./dist/server/src/index.js || touch ./dist/server/src/index.js'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.12.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^19.2.2",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-recaptcha": "^3.6.6",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@swc/core": "^1.3.18",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  }
}
