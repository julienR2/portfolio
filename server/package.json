{
  "name": "nowmad",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ts-node": "TS_NODE_TRANSPILE_ONLY=true ts-node",
    "dev": "nodemon --watch \"./src/**\" --ext \"ts,json\" --exec \"yarn ts-node ./src/index.ts\"",
    "build": "tsc",
    "start": "yarn build && PORT=8000 nodemon --watch \"./build/**\" -x 'node ./build/index.js || touch ./build/index.js'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "puppeteer": "^19.2.2",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-recaptcha": "^3.6.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@swc/core": "^1.3.18",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  }
}
